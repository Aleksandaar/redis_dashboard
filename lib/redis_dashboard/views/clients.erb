<div class="table">
  <table>
    <thead>
      <% for key in (keys = clients.first.keys) %>
        <th><%= key %></th>
      <% end %>
    </thead>
    <% for client in clients %>
      <tr>
        <% for key in keys %>
          <td><%= client[key] %></td>
        <% end %>
      </tr>
    <% end %>
  </table>
</div>
